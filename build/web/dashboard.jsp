    <script>

        function validarNombre(nombre) {
            if (nombre.length < 2) {
                $(".name").removeClass("hide");
                $(".nombre").addClass("has-error");
                $(".error").html("Debe contener por lo menos 2 caracteres");
                $(".nombre").append('<span class="glyphicon glyphicon-exclamation-sign form-control-feedback"></span>');
                return false;
            } else {
                $(".name").addClass("hide");
                $(".nombre").removeClass("has-error");
                $(".nombre span").remove();
                $.get("usuarioControlador",
                        {
                            accion: "verificaAlias",
                            alias_usu: nombre
                        })
                        .done(function (response) {
                            if (response == 1) {
                                $(".name").removeClass("hide");
                                $(".nombre").addClass("has-error");
                                $(".error").html("Debe contener por lo menos 2 caracteres");
                                $(".nombre").append('<span class="glyphicon glyphicon-exclamation-sign form-control-feedback"></span>');
                                return false;
                            } else {
                                $(".name").addClass("hide");
                                $(".nombre").removeClass("has-error");
                                $(".nombre span").remove();
                                return true;
                            }
                        });
            }
        }

        function validarPass(password, confirm) {
            if (password.length < 4) {
                $(".pwd span").remove();
                $(".confirm span").remove();
                $(".pass").addClass("hide");
                $(".length").removeClass("hide");
                $(".pwd").addClass("has-error");
                $(".pwd").append('<span class="glyphicon glyphicon-exclamation-sign form-control-feedback"></span>');
                return false;
            } else {
                $(".length").addClass("hide");
                $(".pwd").removeClass("has-error");
                $(".confirm").removeClass("has-error");
                if (password == confirm) {
                    $(".pwd span").remove();
                    $(".confirm span").remove();
                    $(".pwd").addClass("has-success");
                    $(".confirm").addClass("has-success");
                    $(".pass").addClass("hide");
                    $(".pwd").append('<span class="glyphicon glyphicon-ok form-control-feedback"></span>');
                    $(".confirm").append('<span class="glyphicon glyphicon-ok form-control-feedback"></span>');
                    return true;
                } else {
                    $(".pwd span").remove();
                    $(".confirm span").remove();
                    $(".pwd").addClass("has-error");
                    $(".confirm").addClass("has-error");
                    $(".pass").removeClass("hide");
                    $(".pwd").append('<span class="glyphicon glyphicon-exclamation-sign form-control-feedback"></span>');
                    $(".confirm").append('<span class="glyphicon glyphicon-exclamation-sign form-control-feedback"></span>');
                    return false;
                }
            }
        }

        function validarSelect(value) {
            if (value == "" || value == null) {
                $(".emp").removeClass("hide");
                return false;
            } else {
                $(".emp").addClass("hide");
                return true;
            }
        }
    </script>
